        "build-all": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "yarn build mfe-dashboard --prod",
              "yarn build mfe-counter --prod",
              "yarn build mfe-scoreboard --prod",
              "yarn build mfe-toh --prod"
            ],
            "parallel": true
          }
        },
        "serve-all": {
          "builder": "@nrwl/workspace:run-commands",
